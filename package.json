{
    "name": "theme-material",
    "type": "theme",
    "extend": "ext-theme-neptune",
    "creator": "Steffen Hiller",
    "summary": "Ext JS Material Design Theme",
    "detailedDescription": "The Material Design theme provides styles and custom components that follow Google's Material Design guideline.",
    "version": "0.1.0",
    "compatVersion": "0.1.0",
    "format": "1",
    "slicer": {
        "js": [
            {
                "path": "${package.dir}/sass/example/custom.js",
                "isWidgetManifest": true
            }
        ]
    },
    "output": "${package.dir}/build",
    "local": true,
    "requires": []
}